import java.util.Scanner;
class Practice{
	public static void main(String args[]){
	Scanner sc = new Scanner(SYstem.in);
	System.out.println("Enter the first number");
	int n = sc.nextInt();
	System.out.println("Enter the second number");
	int m = sc.nextInt();

	int a = n + m;
	System.out.println(a);
	}
}
